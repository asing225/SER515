<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>SER515Project_Team3</display-name>

	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>com.asu.ser515.controller.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>QuizController</servlet-name>
		<servlet-class>com.asu.ser515.controller.TeacherServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>QuizController</servlet-name>
		<url-pattern>/quiz</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>StudentController</servlet-name>
		<servlet-class>com.asu.ser515.controller.StudentServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>StudentController</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AdminController</servlet-name>
		<servlet-class>com.asu.ser515.controller.AdminServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AdminController</servlet-name>
		<url-pattern>/admin</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
